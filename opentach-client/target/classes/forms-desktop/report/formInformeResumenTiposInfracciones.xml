						<CampoFechaNoEditable
							attr="FILTERFECINI"
							text="DESDE"
							align="left"
							required="yes"
							datevisible="no"
							dim="no"
							labelsize="10" />
						<CampoFechaNoEditable
							attr="FILTERFECFIN"
							text="HASTA"
							align="left"
							required="yes"
							datevisible="no"
							dim="no"
							labelsize="10" />
					</Column>
				</ColumnRadioButtonGroup>
				<Column />
			</Column>
		</Column>
	</Row>
	<Row expand="yes">
	    <Column>
				<Row expand="yes">
					<Table entity="EInformeResumenTiposInfracciones" keys="CIF;NUMREQ;IDCONDUCTOR" parentkey="CIF" otherkeys="FILTERFECINI;FILTERFECFIN;CG_CONTRATO"
