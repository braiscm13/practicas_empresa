					valign="center">
					<ReferenceExtDataField
						entity="EDfEmp"
						text="EMPRESA"
						labelsize="12"
						attr="CIF"
						dim="text"
						cod="CIF"
						cols="CIF;NOMB"
						visiblecols="CIF;NOMB"
						descriptioncols="NOMB"
						required="yes"
						csize="10" />
				</Column>
			</Column>
		</Column>
		<Column
			expandlast="yes"
			expand="no">
			<Column>
				<ColumnRadioButtonGroup
