<?xml version="1.0" encoding="ISO-8859-1" ?>
<FormExt
	entity="EDfEmp"
	buttons="no"
	title="ELIMINAR_EMPRESA"
	keys="CIF">
	<FormHeader standardbuttons="no" />

	<ObjectDataField
		attr="CIF"
		visible="no" />
	<Row>
		<Column
			align="left"
			expand="yes">
			<Column
				expand="no"
				align="left">
				<Row height="20" />
				<Row>
					<Label
						text="BAJA_EMPRESA"
						expand="yes"></Label>
				</Row>
				<Row height="20" />
				<Row>
					<CheckDataField
						attr="BORRAR_EMPRESA"
						selected="no"
						labelposition="right" />
				</Row>
				<Row height="20" />
			</Column>
		</Column>
	</Row>
	<Row align="center">
		<Button
			icon="com/ontimize/gui/images/ok.png"
			key="aceptar"
			text="ACEPTAR"
			tip="ACEPTAR" />
		<Button
			icon="com/ontimize/gui/images/cancel.png"
			key="cancelar"
			text="CANCELAR"
			tip="CANCELAR" />
	</Row>
</FormExt>