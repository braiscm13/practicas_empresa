	CDVCONDUCTORES_EMP.APELLIDOS , 
	EMAIL , 
	EXPIRED_DATE_TRJCONDU ,
	NOMBRE_EMPRESA
FROM 
	CDVCONDUCTORES_EMP , 
	CDCONDUCTOR_CONT
WHERE
	CDVCONDUCTORES_EMP.IDCONDUCTOR =  CDCONDUCTOR_CONT.IDCONDUCTOR AND
	CG_CONTRATO = $P{numreq} and 
	CIF = $P{cif}
