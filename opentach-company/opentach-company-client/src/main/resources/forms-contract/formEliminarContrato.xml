				</Row>
			</Column>
			<Row/>
		</Row>
		<Row align="center">
			<Button
				icon="com/ontimize/gui/images/ok.png"
				key="aceptar"
				text="ACEPTAR"
				tip="ACEPTAR" />
			<Button
				icon="com/ontimize/gui/images/cancel.png"
				key="cancelar"
				text="CANCELAR"
				tip="CANCELAR" />
